{
  "name": "extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:service_worker": "tsc -p ./src/services/tsconfig.json && TYPE=service_worker vite build",
    "build:content_script": "tsc && TYPE=content_script vite build",
    "build": "pnpm run \"/^build:.*/\"",
    "dev:build": "pnpm run build --mode development",
    "dev:prod": "pnpm run build && concurrently --kill-others \"pnpm run build --watch\"",
    "dev": "pnpm run dev:build && concurrently --kill-others \"node script/dev.mjs\" \"pnpm run dev:build --watch\"",
    "dev:1": "USERS=1 pnpm run dev",
    "dev:3": "USERS=3 pnpm run dev",
    "dev:4": "USERS=4 pnpm run dev",
    "test": "TYPE=content_script pnpm vitest"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@tsparticles/engine": "^3.5.0",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "firebase": "^10.12.2",
    "framer-motion": "^11.5.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.474.0",
    "react": "catalog:react18",
    "react-dom": "catalog:react18",
    "react-resizable": "^3.0.5",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/react": "^16.2.0",
    "@types/chrome": "^0.0.268",
    "@types/lodash": "^4.17.14",
    "@types/node": "^20.13.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-resizable": "^3.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "chokidar": "^4.0.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.38",
    "puppeteer": "^24.1.0",
    "tailwindcss": "^3.4.3"
  }
}
