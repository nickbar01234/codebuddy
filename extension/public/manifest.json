{
  "manifest_version": 3,
  "name": "CodeBuddy",
  "description": "Leetcode together",
  "version": "1.0.0",
  "action": {},
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["assets/index.js", "contentScript.js"],
      "css": ["assets/index.css"],
      "matches": ["https://leetcode.com/problems/*"]
    }
  ]
}
